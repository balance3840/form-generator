{
  "name": "@restrella/form-generator",
  "version": "0.0.9",
  "description": "Form generator using JSON schema",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/form-generator/form-generator.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/balance3840/form-generator"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs --experimentalImportInjection",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^4.12.0",
    "lodash": "^4.17.21",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@stencil/sass": "^3.0.9",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.11.0"
  },
  "license": "MIT"
}
